

<form asp-controller="Test" asp-action="CreateFromFile" enctype="multipart/form-data" id="formCreateFromFile">
    <div>
        <div class="form-group" style="margin-left:7rem;">
            <label for="fileInput">Выберите файл:</label>
            <input type="file" class="form-control-file" name="file" accept=".csv" required>
        </div>
        <button type="submit" class="btn btn-primary " style="margin-left:12rem;">Загрузить</button>
    </div>
</form>